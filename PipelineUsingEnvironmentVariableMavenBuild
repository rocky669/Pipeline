pipeline{
    agent any
    enviroment{
        PATH="C:\\DevTools\\apache-maven-3.9.1\\bin":$PATH
    }
    stages{
        stage("Github fetch data"){
            steps{
                git 'https://github.com/rocky669/time-tracker'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
